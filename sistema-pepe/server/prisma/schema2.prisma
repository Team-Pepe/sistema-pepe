generator client2 {
  provider = "prisma-client-js"
  output   = "./generated/client2"
}

datasource db2 {
  provider  = "postgresql"
  url       = env("DATABASE_URL_DB2")
  directUrl = env("DIRECT_URL_DB2")
}

model LoginUsers {
  id       Int    @id @default(autoincrement())
  email    String @unique @db2.VarChar(100)
  password String @db2.Text

  @@map("login_users")
}